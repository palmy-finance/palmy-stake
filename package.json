{
  "name": "palmy-stake",
  "version": "1.0.2",
  "description": "contracts related with staking",
  "files": [
    "contracts",
    "artifacts"
  ],
  "scripts": {
    "run-env": "npm i && tail -f /dev/null",
    "hardhat": "hardhat",
    "hardhat-kovan": "cross-env hardhat --network kovan",
    "hardhat-rinkeby": "cross-env hardhat --network rinkeby",
    "hardhat-main": "cross-env hardhat --network main",
    "hardhat:tenderly": "cross-env hardhat --network tenderly",
    "hardhat-oasys": "cross-env hardhat --network oasys",
    "hardhat-testnet": "cross-env hardhat --network testnet",
    "hardhat help": "hardhat help",
    "compile": "SKIP_LOAD=true hardhat compile",
    "compile:force": "npm run compile -- --force",
    "compile:force:quiet": "npm run compile:force -- --quiet",
    "test": "npm run compile:force:quiet && hardhat test test/__setup.spec.ts test/IncentivesController/*.spec.ts test/StakedOas/*.spec.ts test/StakedOasV2/*.spec.ts",
    "test:ci": "npm run compile:force:quiet && npm run test-pei && npm run test-psi && npm run test-psi2 && npm run test-bpt",
    "test-pei": "npm run test test/__setup.spec.ts test/IncentivesController/*.spec.ts",
    "test-psi": "npm run test test/__setup.spec.ts test/StakedOas/*.spec.ts",
    "test-psi2": "npm run test test/__setup.spec.ts test/StakedOasV2/*.spec.ts",
    "test-bpt": "npm run compile:force:quiet && FORKING_BLOCK=11730175 MAINNET_FORK=true hardhat test test/StakedBPT/create-bpt-and-stakebpt.fork.ts",
    "test-proposal": "MAINNET_FORK=true hardhat test ./test-fork/stakingExtensionProposal.spec.ts",
    "test-proposal:tenderly": "TENDERLY=true npm run hardhat:tenderly -- test ./test-fork/stakingExtensionProposal.spec.ts",
    "test-StakedOas": "npm run test test/__setup.spec.ts test/StakedOasV2/StakedOas-basic.spec.ts",
    "proposal-extend-stkoas:tenderly": "TENDERLY=true npm run hardhat:tenderly -- proposal-stk-oas-extension:tenderly",
    "proposal-extend-stkoas:tenderly:defender": "TENDERLY=true npm run hardhat:tenderly -- proposal-stk-oas-extension:tenderly --defender",
    "coverage": "npm run compile && npx hardhat coverage --testfiles \"test/**/*.spec.ts\"",
    "dev:deployment": "hardhat dev-deployment",
    "kovan:deployment": "npm run hardhat-kovan -- common-deployment --verify",
    "main:deployment": "npm run hardhat-main -- common-deployment --verify",
    "oasys:deployment": "npm run hardhat-oasys -- common-deployment",
    "testnet:deployment": "npm run hardhat-testnet -- common-deployment",
    "testnet:initialization": "npm run hardhat-testnet -- common-initialization",
    "oasys:initialization": "npm run hardhat-oasys -- common-initialization",
    "oasys:verification": "npm run hardhat-oasys -- verification",
    "testnet:verification": "npm run hardhat-testnet -- verification",
    "oasys:print-configs": "npm run hardhat-oasys -- print-configs",
    "testnet:print-configs": "npm run hardhat-testnet -- print-configs",
    "prettier:check": "npx prettier -c 'tasks/**/*.ts' 'contracts/**/*.sol' 'helpers/**/*.ts'  'test/**/*.ts'",
    "prettier:write": "prettier --write 'tasks/**/*.ts' 'contracts/**/*.sol' 'helpers/**/*.ts'  'test/**/*.ts'",
    "ci:clean": "rm -rf types/ cache/ artifacts/",
    "prepublishOnly": "npm run compile"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "3.1.0",
    "@tenderly/hardhat-tenderly": "^1.1.0-beta.5",
    "@typechain/ethers-v5": "^6.0.5",
    "@typechain/hardhat": "^1.0.1",
    "@types/chai": "4.2.11",
    "@types/lowdb": "1.0.9",
    "@types/mocha": "7.0.2",
    "@types/node": "14.0.5",
    "bignumber.js": "9.0.0",
    "chai": "4.2.0",
    "chai-bignumber": "3.0.0",
    "defender-relay-client": "^1.5.1",
    "eth-sig-util": "^3.0.0",
    "ethereum-waffle": "^3.2.1",
    "ethereumjs-util": "^7.0.7",
    "ethers": "^5.0.8",
    "cross-env": "^7.0.3",
    "hardhat": "^2.2.1",
    "husky": "^4.3.8",
    "lowdb": "1.0.0",
    "moment": "^2.27.0",
    "prettier": "^2.0.5",
    "prettier-plugin-solidity": "^1.0.0-beta.1",
    "pretty-quick": "^2.0.1",
    "solidity-coverage": "^0.7.16",
    "tmp-promise": "^3.0.2",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typechain": "^3.0.0",
    "typescript": "^4.1.2",
    "dotenv": "^16.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'contracts/**/*.sol' --pattern 'helpers/**/*.ts'  --pattern 'test/**/*.ts' --pattern 'tasks/**/*.ts'"
    }
  },
  "author": "HorizonX.tech",
  "license": "AGPL-3.0",
  "dependencies": {},
  "keywords": [
    "palmy",
    "stake",
    "token",
    "safety",
    "module",
    "ethereum"
  ]
}
